antigen use oh-my-zsh

# oh-my-zsh plugins
hash git 2>/dev/null && antigen bundle git
hash pip 2>/dev/null && antigen bundle pip
hash go 2>/dev/null && antigen bundle golang
hash docker 2>/dev/null && antigen bundle docker
hash python 2>/dev/null && antigen bundle python
hash vagrant 2>/dev/null && antigen bundle vagrant
hash tmuxinator 2>/dev/null && antigen bundle tmuxinator
hash virtualenvwrapper.sh 2>/dev/null && antigen bundle virtualenvwrapper
antigen bundle zsh-users/zsh-completions src

antigen bundle vi-mode

# Non oh-my-zsh plugins
antigen bundle zsh-users/zsh-history-substring-search
antigen bundle zsh-users/zsh-syntax-highlighting

# Platform dependant bundles
if [[ `uname` == 'Linux' ]]
then
    antigen bundle command-not-found
elif [[ `uname` == 'Darwin' ]]
then
    # Only enable brew plugin if brew exists
    if hash brew 2>/dev/null; then
        antigen bundle brew
    fi
fi

# Install ringwood theme. Don't verify the SSL cert for now.
export GIT_SSL_NO_VERIFY=1
antigen theme https://peculiar.arndt.ca/darndt/zsh_themes.git ringwood
export GIT_SSL_NO_VERIFY=0

antigen apply

